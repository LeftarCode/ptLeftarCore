{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug x64",
            "type": "cppvsdbg",
            "request": "launch",
            "preLaunchTask": "generate-and-build-project-debug",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/out/ptLeftarCore/Debug/ptLeftarCore.exe",
            "console": "externalTerminal",
            "logging": {
                "moduleLoad": false,
                "trace": true
            },
            "visualizerFile": "${workspaceFolder}/my.natvis"
        },
        {
            "name": "Release x64",
            "type": "cppvsdbg",
            "request": "launch",
            "preLaunchTask": "generate-and-build-project-release",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/out/ptLeftarCore/Release/ptLeftarCore.exe",
            "console": "externalTerminal",
            "logging": {
                "moduleLoad": false,
                "trace": false
            },
            "visualizerFile": "${workspaceFolder}/my.natvis"
        }
    ]
}